unset imap_passive

set alias_file= ~/.mutt/aliases
set mime_forward=yes
set mime_forward_rest=yes
set imap_keepalive=300
set timeout=10
set mail_check=10
set use_from=yes
set use_envelope_from=yes
set realname='Yvon Cognard'
set sendmail="/usr/bin/msmtp"
set text_flowed=yes
set ssl_starttls=no
set header_cache="~/.hcache"
set sort_alias=alias
set reverse_alias=yes
set sort=threads
set sort_aux=last-date-received
set delete=yes
set confirmappend=no
set include=yes
set markers=no
set index_format="%Z %{%d/%m/%y} %-15.15L %s"

source $alias_file
source ~/.my_mutt

color index     color190 default "~F"
color index     color160 default "~N"
color index     color160 default "~O"
color status    white color88
color indicator black color15
